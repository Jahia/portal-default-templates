                                       jcr:title="3 Columns"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                     jcr:primaryType="jnt:bootstrapFullComponent"
                                     layout="3_6_3"
                                     rowType="predefined"/>
                                </pagecontent>
                            </_x0033_col>
                            <content-template j:applyOn="jnt:content"
                                    j:hiddenTemplate="true"
                                    j:priority="1"
                                    jcr:primaryType="jnt:contentTemplate">
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                                </pagecontent>
                            </content-template>
                            <single-news j:applyOn="jnt:news"
                               j:defaultTemplate="true"
                               j:hiddenTemplate="true"
                               j:priority="100"
                               j:view="default"
                               jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="single-news"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <bootstrap-container fluid="false" jcr:primaryType="jnt:bootstrapContainer">
                                        <bootstrap-container jcr:primaryType="jnt:contentList">
                                            <bootstrap-row fluid="false" jcr:primaryType="jnt:bootstrapRow">
                                                <bootstrap-column jcr:primaryType="jnt:bootstrapColumn" offset="1" span="9">
                                                    <bootstrap-column jcr:primaryType="jnt:contentList">
                                                        <bootstrap-breadcrumb displayCurrentPage="true"
                                                             displayHome="true"
                                                             displayLinkOnCurrentPage="false"
                                                             displayOnFirstLevel="false"
                                                             divider="/"
                                                             jcr:primaryType="jnt:bootstrapBreadcrumb"/>
                                                    </bootstrap-column>
                                                </bootstrap-column>
                                            </bootstrap-row>
                                            <bootstrap-row-1 fluid="false" jcr:primaryType="jnt:bootstrapRow">
                                                <bootstrap-column jcr:primaryType="jnt:bootstrapColumn" offset="0" span="13">
                                                    <bootstrap-column jcr:primaryType="jnt:contentList">
                                                        <main-resource-display j:mainResourceView="portal" jcr:primaryType="jnt:mainResourceDisplay"/>
                                                    </bootstrap-column>
                                                </bootstrap-column>
                                            </bootstrap-row-1>
                                        </bootstrap-container>
                                    </bootstrap-container>
                                </pagecontent>
                            </single-news>
                            <header jcr:primaryType="jnt:contentList">
                                <home inverse="true"
                           jcr:primaryType="jnt:bootstrapNavBar"
                           option="navbar-fixed-top"
