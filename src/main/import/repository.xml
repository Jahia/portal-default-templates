<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <portal-default-templates j:dependencies="bookmarks bootstrap3-components bootstrap3-core contact default event facets portal-core portal-user-portal search teasers"
                                j:installedModules="portal-default-templates"
                                j:modulePriority="0"
                                j:moduleType="templatesSet"
                                j:title="Portal factory - default templates"
                                jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:view="portal" jcr:primaryType="jnt:template">
                        <footer jcr:primaryType="jnt:contentList">
                            <footer-container columnsType="predefinedColumns"
                                    grid="12"
                                    gridLayout="full-width"
                                    jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                    jcr:primaryType="bootstrap3nt:columns">
                                <footer-container jcr:primaryType="jnt:contentList">
                                    <rich-text jcr:primaryType="jnt:bigText">
                                        <j:translation_en jcr:language="en"
                                             jcr:primaryType="jnt:translation"
                                             text="&lt;p&gt;&amp;copy; 2002 - 2018 Jahia Solutions Group SA tous droits r&amp;eacute;serv&amp;eacute;s&lt;/p&gt;&#xA;"/>
                                    </rich-text>
                                </footer-container>
                            </footer-container>
                        </footer>
                        <portal j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="portal"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <section--container--row--grid columnsType="predefinedColumns"
                                                    grid="12"
                                                    gridLayout="full-width"
                                                    jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                    jcr:primaryType="bootstrap3nt:columns">
                                    <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                        <portal-toolbar j:view="menuLogin"
                                           jcr:mixinTypes="jmix:renderable"
                                           jcr:primaryType="jnt:portalToolbar">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="portal toolbar"/>
                                        </portal-toolbar>
                                    </section--container--row--grid-main>
                                </section--container--row--grid>
                                <portal_x0020_content jcr:primaryType="jnt:area"/>
                                <portal-widget-zap jcr:primaryType="jnt:portalWidgetsZap">
                                    <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="portal widget zap"/>
                                </portal-widget-zap>
                            </pagecontent>
                            <_x0031_-column j:applyOn="jnt:portalTab"
                                  j:defaultTemplate="false"
                                  j:hiddenTemplate="false"
                                  jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="1 Column"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="12"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0031_-column>
                            <_x0032_-columns-4--8 j:applyOn="jnt:portalTab"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Columns (4 / 8)"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="4_8"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-side jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-side>
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0032_-columns-4--8>
                            <_x0032_-columns-8--4 j:applyOn="jnt:portalTab"
                                        j:defaultTemplate="false"
                                        j:hiddenTemplate="false"
                                        jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 Columns (8 / 4)"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="8_4"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                        <section--container--row--grid-side jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-side>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0032_-columns-8--4>
                            <_x0033_-columns j:applyOn="jnt:portalTab"
                                   j:defaultTemplate="false"
                                   j:hiddenTemplate="false"
                                   jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="3 Columns"/>
                                <portal_x0020_content jcr:primaryType="jnt:contentList">
                                    <section--container--row--grid columnsType="predefinedColumns"
                                                       grid="4_4_4"
                                                       gridLayout="full-width"
                                                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                       jcr:primaryType="bootstrap3nt:columns">
                                        <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-main>
                                        <section--container--row--grid-side jcr:primaryType="jnt:contentList">
                                            <col2 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-side>
                                        <section--container--row--grid-extra jcr:primaryType="jnt:contentList">
                                            <col3 jcr:primaryType="jnt:portalArea"/>
                                        </section--container--row--grid-extra>
                                    </section--container--row--grid>
                                </portal_x0020_content>
                            </_x0033_-columns>
                        </portal>
                        <site j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="site"/>
                            <_x0031_col jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="1 Column"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent columnsType="predefinedColumns"
                                     grid="12"
                                     gridLayout="full-width"
                                     jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                     jcr:primaryType="bootstrap3nt:columns"/>
                                </pagecontent>
                            </_x0031_col>
                            <_x0032_cols j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="2 columns"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent columnsType="predefinedColumns"
                                     grid="4_8"
                                     gridLayout="full-width"
                                     jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                     jcr:primaryType="bootstrap3nt:columns"/>
                                </pagecontent>
                            </_x0032_cols>
                            <_x0033_col jcr:primaryType="jnt:pageTemplate">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="3 Columns"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent columnsType="predefinedColumns"
                                     grid="3_6_3"
                                     gridLayout="full-width"
                                     jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                     jcr:primaryType="bootstrap3nt:columns"/>
                                </pagecontent>
                            </_x0033_col>
                            <content-template j:applyOn="jnt:content"
                                    j:hiddenTemplate="true"
                                    j:priority="1"
                                    jcr:primaryType="jnt:contentTemplate">
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                                </pagecontent>
                            </content-template>
                        </site>
                    </base>
                </templates>
                
                <home j:isHomePage="true"
               j:templateName="2cols"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                    <pagecontent-side jcr:primaryType="jnt:contentList">
                        <bootstrap-layer-container-rows-a columnsType="predefinedColumns"
                                                 grid="12"
                                                 gridLayout="full-width"
                                                 jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                                                 jcr:primaryType="bootstrap3nt:columns">
                            <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList"/>
                            <row columnsType="predefinedColumns"
                       grid="12"
                       gridLayout="full-width"
                       jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:predefinedColumns"
                       jcr:primaryType="bootstrap3nt:columns">
                                <row jcr:primaryType="jnt:contentList">
                                    <users-portals-list jcr:primaryType="jnt:userPortals"/>
                                </row>
                            </row>
                            <bootstrap-layer-container-rows-a-main jcr:primaryType="jnt:contentList">
                                <login-form j:displayLoginButton="true"
                                 j:displayRememberMeButton="true"
                                 jcr:primaryType="jnt:loginForm"/>
                                <users-portals-list jcr:primaryType="jnt:userPortals"/>
                            </bootstrap-layer-container-rows-a-main>
                        </bootstrap-layer-container-rows-a>
                    </pagecontent-side>
                </home>
                <search-results j:templateName="1col" jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search results"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <advanced-search-form jcr:primaryType="jnt:advancedSearchForm"/>
                        <search-results autoSuggest="true"
                               autoSuggestHitCount="2"
                               autoSuggestMaxTermCount="1"
                               autoSuggestMinimumHitCount="2"
                               jcr:primaryType="jnt:searchResults"/>
                    </pagecontent>
                </search-results>
            </portal-default-templates>
    </modules>
</content>