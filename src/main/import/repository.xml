                                    <bootstrapmenulanguageswitcher jcr:primaryType="jnt:bootstrapMenuLanguageSwitcher"
                                                       position="right"
                                                       typeOfDisplay="isoLocaleCountryCode"/>
                                </home>
                            </header>
