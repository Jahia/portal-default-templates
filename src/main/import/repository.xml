                                                          jcr:primaryType="jnt:bootstrapFullComponent"
                                                          layout="8_4"
                                                          rowType="predefined">
                                        <bootstrap-layer-container-rows-a jcr:primaryType="jnt:contentList">
                                            <col1 jcr:primaryType="jnt:portalArea"/>
                                    </bootstrap-layer-container-rows-a>
